<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://appengine.google.com/ns/1.0 http://googleappengine.googlecode.com/svn/branches/1.2.1/java/docs/appengine-web.xsd">
	<application>zimmem-wiki</application>
    <version>20110710</version>

	<system-properties>
		<property name="java.util.logging.config.file" value="WEB-INF/classes/logging.properties" />
		<property name="mircowiki.metadata.title" value="mirco-wiki" />
		<property name="velocity.dateformat.default" value="yyyy-MM-dd" />
	</system-properties>

	<sessions-enabled>true</sessions-enabled>
	<static-files>
		<include path="/static/**.css" />
		<include path="/favicon.ico" />
		<include path="/static/**.js" />
		<include path="/robots.txt" />
		<include path="/*.html" />
	</static-files>

</appengine-web-app>
